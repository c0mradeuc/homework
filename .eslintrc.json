{
    "env": {
        "browser": false,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "globals": {
        "console": true,
        "process": true
    },
    "rules": {
        "require-jsdoc": 0,
        "indent": ["error", 2],
        "object-curly-spacing": 0,
        "no-undef": "error",
        "semi": ["warn", "never"],
        "new-cap": "off",
        "comma-dangle": "off",
        "max-len": ["warn", 200],
        "brace-style": ["warn", "stroustrup"]
    }
}
